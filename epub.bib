  year      =  2020,
  url       = "http://dx.doi.org/10.25304/rlt.v28.2329",
  keywords  = "EPubs Books;2\_Upgrade MAY 2023;Journal Articles;Notion
               Catalogued;AEC Cited Lit for Jacqui;\_BibTex to MD
               Git;AEC;ePub;\_Mark Up;Cited for Frontiers;\_Cataloged",
  doi       = "10.25304/rlt.v28.2329",
